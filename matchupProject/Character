public abstract class Character {

    public String charName;
    private Archetype type;
    private MoveSet[] moves;

    public void setCharacterMoveSet(MoveSet[] moves){

        this.moveSet[] = new moveSet[];

    }

    public void printCharacterMoveSet(Character character) {

        System.out.println(this.moveSet);

    }

    public Character (String charName, Archetype type, MoveSet[] moveSet) {

        this.charName = charName;
        this.type = type;
        this.moveSet = moveSet;

    }

    public Character (String charName, Archetype type) {

        this.charName = charName;
        this.type = type;

    }

    public Character (String charName) {

        this.charName = charName;

    }

    public String toString() {

        return this.charName;

    }

}